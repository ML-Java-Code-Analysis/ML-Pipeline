[DATABASE]
dialect = mysql
name = gtsoog
user = root
user_password = root
host = localhost
port = 3306

[LOGGING]
level = DEBUG
override = True
# file = ml_pipeline.log
# format = %(asctime)s [%(levelname)s] %(message)s
# date_format = %Y.%m.%d %H:%M:%S

[REPOSITORY]
name = elasticsearch

[DATASET]
cache = True
target = sixmonths
train_start = 2015.01.01
train_end = 2015.01.31
test_start = 2015.02.01
test_end = 2015.02.28
features =
    BLOC,
    CLOC,
    PLOC,
    SLOC

[ML]
model = linear_regression
normalize = False